{
    "name": "fl_bench.algorithms.fedrep.FedRep",
    "hyperparameters": {
        "server": {
            "weighted": false
        },
        "client": {
            "batch_size": 10,
            "local_epochs": 10,
            "loss": "CrossEntropyLoss",
            "model": "MNIST_2NN_ED",
            "tau": 5,
            "optimizer": {		
                "lr": 0.01,
                "weight_decay": 0.0001,
                "momentum": 0.9
            },
            "scheduler_kwargs": {
                "step_size": 1, 
                "gamma": 1
            }
        },
        "model": "MNIST_2NN_E"
    }
}