{
    "name": "fedavg",
    "hyperparameters": {
        "server": {
            "weighted": true
        },
        "client": {
            "batch_size": 20,
            "local_epochs": 10,
            "loss": "CrossEntropyLoss",
            "optimizer": {		
                "lr": 0.3
            },
            "scheduler": {
                "step_size": 1, 
                "gamma": 1
            }
        },
        "model": "MNIST_CNN"
    }
}