{
    "name": "fedavg",
    "hyperparameters": {
        "server": {
            "weighted": true
        },
        "client": {
            "batch_size": 50,
            "local_epochs": 5,
            "loss": "CrossEntropyLoss",
            "optimizer": {		
                "lr": 0.15
            },
            "scheduler": {
                "step_size": 5, 
                "gamma": 0.99
            }
        },
        "model": "FedavgCNN"
    }
}