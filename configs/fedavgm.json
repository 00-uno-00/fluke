{
    "name": "l_bench.algorithms.fedavgm.FedAVGM",
    "hyperparameters": {
        "server": {
            "momentum": 0.9
        },
        "client": {
            "batch_size": 10,
            "local_epochs": 10,
            "loss": "CrossEntropyLoss",
            "optimizer": {		
                "lr": 0.01,
                "weight_decay": 0.0001
            },
            "scheduler_kwargs": {
                "step_size": 1, 
                "gamma": 1
            }
        },
        "model": "MNIST_2NN"
    }
}