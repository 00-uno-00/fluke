{
    "name": "fl_bench.algorithms.fednova.FedNova",
    "hyperparameters": {
        "server": {
            "weighted": true
        },
        "client": {
            "batch_size": 64,
            "local_epochs": 5,
            "loss": "CrossEntropyLoss",
            "optimizer": {		
                "lr": 0.01,
                "momentum": 0.9,
                "weight_decay": 0.0001
            },
            "scheduler": {
                "step_size": 1, 
                "gamma": 1
            }
        },
        "model": "MNIST_2NN"
    }
}