{
    "name": "superfed",
    "hyperparameters": {
        "server": {
            "weighted": true
        },
        "client": {
            "batch_size": 10,
            "local_epochs": 10,
            "loss": "CrossEntropyLoss",
            "mode": "mm",
            "mu": 0.01,
            "nu": 2,
            "start_mix": 1,
            "model": "VGG9",
            "optimizer": {		
                "lr": 0.01,
                "weight_decay": 0.0001,
                "momentum": 0.9
            },
            "scheduler": {
                "step_size": 10, 
                "gamma": 0.995
            }
        },
        "model": "VGG9"
    }
}