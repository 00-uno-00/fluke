{
    "name": "fedavg",
    "hyperparameters": {
        "model": "Shakespeare_LSTM",
        "server": {
            "weighted": true
        },
        "client": {
            "batch_size": 10,
            "local_epochs": 5,
            "loss": "CrossEntropyLoss",
            "optimizer": {		
                "lr": 0.8
            },
            "scheduler": {
                "step_size": 10, 
                "gamma": 0.995
            }
        }
    }
}