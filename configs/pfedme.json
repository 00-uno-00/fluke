{
    "name": "fl_bench.algorithms.pfedme.PFedMe",
    "hyperparameters": {
        "server": {
            "beta": 1
        },
        "client": {
            "batch_size": 20,
            "local_epochs": 20,
            "loss": "CrossEntropyLoss",
            "k": 5,
            "optimizer": {		
                "lr": 0.01,
                "lamda": 15,
                "mu": 0.001
            },
            "scheduler": {
                "step_size": 1, 
                "gamma": 1
            }
        },
        "model": "MNIST_2NN"
    }
}