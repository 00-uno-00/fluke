{
    "name": "fl_bench.algorithms.feddyn.FedDyn",
    "hyperparameters": {
        "server": {
            "weighted": false
        },
        "client": {
            "batch_size": 10,
            "local_epochs": 20,
            "loss": "CrossEntropyLoss",
            "alpha": 0.01,
            "optimizer": {		
                "lr": 0.1,
                "weight_decay": 0.0001
            },
            "scheduler": {
                "step_size": 1, 
                "gamma": 1
            }
        },
        "model": "MNIST_2NN"
    }
}