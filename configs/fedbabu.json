{
    "name": "fl_bench.algorithms.fedbabu.FedBABU",
    "hyperparameters": {
        "server": {
            "weighted": true
        },
        "client": {
            "batch_size": 32,
            "local_epochs": 5,
            "loss": "CrossEntropyLoss",
            "model": "MNIST_2NN_Proto",
            "fine_tune_epochs": 5,
            "mode": "full",
            "optimizer": {		
                "lr": 0.01,
                "weight_decay": 0.0001,
                "momentum": 0.9
            },
            "scheduler": {
                "step_size": 1, 
                "gamma": 1
            }
        },
        "model": "MNIST_2NN_E"
    }
}